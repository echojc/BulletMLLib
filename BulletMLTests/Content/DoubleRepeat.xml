<?xml version="1.0" ?>

<bulletml type="vertical"
          xmlns="http://www.asahi-net.or.jp/~cs8k-cyu/bulletml">

	<action label="top">
		<repeat>
			<times>2</times>
			<actionRef label="circle">
			</actionRef>
		</repeat>
		<vanish/>
	</action>
	
	<action label="circle">
		<repeat>
			<times>10</times>
			<action>
				<fire label="testFire">
					<speed type="sequence">1</speed>
					<bullet label="test">
						<speed>1</speed>
					</bullet>
				</fire>
			</action>
		</repeat>
	</action>

</bulletml>
